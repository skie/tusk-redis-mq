{
    "name": "tusk/tusk-redis-mq",
    "type": "library",
    "description": "A simple producer/consumer and RPC message queue on top of Redis and the equally fantastic Predis",
    "keywords": ["redis", "message", "queue", "predis"],
    "homepage": "http://github.com/tusk/tusk-redis-mq",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernard van Niekerk",
            "email": "frizzy@paperjaw.com"
        }
    ],
    "require-dev": {
        "predis/predis": "0.7.*"
    },
    "require": {
        "php": ">=5.3.2"
    },
    "suggest": {
        "predis/predis": "If you want to use predis (currently only v0.7.x is supported)."
    },
    "autoload": {
        "psr-0": {"Tusk\\RedisMq": "src/"}
    },
    "target-dir": "Tusk/RedisMq"
}